<?xml version="1.0" encoding="UTF-8"?>
<web-app
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
    version="3.0">
  
  
  <servlet>
    <servlet-name>bayesianServlet</servlet-name>
    <servlet-class>web.BayesianServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bayesianServlet</servlet-name>
    <url-pattern>/bayesianServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>presentOptions</servlet-name>
    <servlet-class>web.PresentDynamicContentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>presentOptions</servlet-name>
    <url-pattern>/presentOptions</url-pattern>
  </servlet-mapping>
  
</web-app>